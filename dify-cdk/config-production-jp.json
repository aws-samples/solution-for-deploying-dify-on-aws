{
  "isChinaRegion": false,
  "deployLangfuse": false,
  "dify": {
    "version": "1.7.2",
    "pluginDaemon": {
      "enabled": true,
      "storageSize": "20Gi"
    },
    "migration": {
      "enabled": false,
      "skipPluginMigration": true
    },
    "dbMigration": {
      "enabled": true
    }
  },
  "network": {
    "useExistingVpc": false,
    "vpcCidr": "10.0.0.0/16",
    "maxAzs": 3
  },
  "domain": {
    "useCloudfront": true,
    "domainName": "dify.example.jp",
    "hostedZoneId": "YOUR_HOSTED_ZONE_ID",
    "cloudfront": {
      "enabled": true,
      "domainName": "dify.example.jp",
      "aliases": ["www.dify.example.jp"],
      "priceClass": "PriceClass_200",
      "waf": {
        "enabled": true,
        "rateLimit": 5000,
        "geoRestriction": {
          "restrictionType": "whitelist",
          "locations": ["JP", "US", "SG", "KR", "HK", "TW"]
        }
      },
      "cachePolicy": {
        "defaultTTL": 300,
        "maxTTL": 86400,
        "minTTL": 0
      },
      "logging": {
        "enabled": true,
        "prefix": "cloudfront-logs/"
      },
      "originShield": {
        "enabled": true,
        "region": "ap-northeast-1"
      }
    }
  },
  "cluster": {
    "useExistingCluster": false,
    "clusterName": "dify-eks-jp",
    "version": "1.33",
    "vpcSubnetIds": [],
    "managedNodeGroups": {
      "app": {
        "desiredSize": 3,
        "minSize": 2,
        "maxSize": 6,
        "instanceType": "m8g.xlarge",
        "diskSize": 60,
        "workerNodeSubnetIds": []
      }
    },
    "eksClusterName": "dify-eks-jp"
  },
  "s3": {
    "removeWhenDestroyed": false,
    "useAccessKeyAuth": false
  },
  "postgresSQL": {
    "postgresFullVersion": "16.4",
    "postgresMajorVersion": "16",
    "instanceType": "db.t4g.large",
    "dbName": "dify",
    "dbCredentialUsername": "postgres",
    "dbCredentialPassword": "",
    "backupRetention": 7,
    "storageSize": 200,
    "removeWhenDestroyed": false,
    "subnetIds": [],
    "multiAz": {
      "enabled": true,
      "subnetGroupName": ""
    }
  },
  "redis": {
    "engineVersion": "7.0",
    "nodeType": "cache.t4g.small",
    "readReplicas": 1,
    "subnetIds": [],
    "multiAZ": {
      "enabled": true,
      "subnetGroupName": ""
    }
  },
  "openSearch": {
    "enabled": true,
    "masterUserName": "admin",
    "masterUserPassword": "",
    "multiAz": {
      "enabled": true,
      "azCount": 3
    },
    "subnetIds": [],
    "capacity": {
      "dataNodes": 3,
      "dataNodeInstanceType": "t3g.medium.search"
    },
    "dataNodeSize": 50
  }
}